  "keywords": [
    "documentation",
    "generation",
    "markdown"
  ],
  "license": "Apache 2",
  "minimum-stability": "dev",
  "autoload": {
    "files": [
      "helpers.php"
    ],
    "psr-4": {
      "Shalvah\\Pastel\\": "src/"
    }
  },
  "bin": [
    "pastel"
  ],
  "require": {
    "php": ">=7.3",
    "mnapoli/silly": "~1.0",
    "illuminate/view": "^6.0|^7.0",
    "mnapoli/front-yaml": "^1.6 ",
    "windwalker/renderer": "3.*",
    "league/flysystem": "^2.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  }
}
